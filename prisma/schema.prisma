generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "windows"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Document {
  id            String   @id @default(uuid())
  originalName  String
  mimeType      String
  fileSize      Int
  s3Key         String
  extractedText String?
  summary       String?
  docType       String?
  metadata      Json?
  createdAt     DateTime @default(now())
}
